  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/server/app.js -m prod",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "node webpack/clean.js",
    "build:server": "tsc",
    "build:client": "webpack -p --progress --config webpack/webpack-prod.config",
    "build:prod": "npm run build && ren build app",
    "start:dev": "concurrently --raw \"nodemon ./server/app.ts -m dev\" \"npm run webpack-dev-server\"",
    "start:dev:debug": "concurrently --raw \"tsc --watch\" \"npm run webpack-dev-server\"",
    "test": "jest --forceExit",
    "test-ci": "npm test && cat ./coverage/lcov.info | coveralls",
    "webpack-dev-server": "webpack-dev-server --config webpack/webpack-dev.config"
  },
